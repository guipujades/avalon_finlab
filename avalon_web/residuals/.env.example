# Avalon Fund Tracker Configuration
# Copy this file to .env and fill in your values

# Environment (development, staging, production)
AVALON_ENVIRONMENT=development

# BTG API Configuration
AVALON_BTG_API_KEY=your_btg_api_key_here
AVALON_BTG_API_SECRET=your_btg_api_secret_here

# MetaTrader 5 Configuration
AVALON_MT5_LOGIN=your_mt5_login
AVALON_MT5_PASSWORD=your_mt5_password
AVALON_MT5_SERVER=your_mt5_server

# Database Configuration
AVALON_DB_TYPE=sqlite
AVALON_DB_HOST=localhost
AVALON_DB_PORT=5432
AVALON_DB_NAME=avalon_fund
AVALON_DB_USER=avalon_user
AVALON_DB_PASSWORD=your_db_password

# Security Configuration
AVALON_SECRET_KEY=generate_a_strong_secret_key_here
AVALON_JWT_SECRET=generate_another_secret_key_here